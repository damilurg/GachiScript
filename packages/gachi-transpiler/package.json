{
  "name": "@gachiscript/transpiler",
  "version": "1.0.0",
  "description": "ðŸ”¥ GachiScript Transpiler - Transform between JS/TS and GachiScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "gachi": "dist/cli.js",
    "gachify": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts src/cli.ts --watch --format esm",
    "build": "tsup src/index.ts src/cli.ts --dts --format esm",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "gachiscript",
    "transpiler",
    "ast",
    "typescript",
    "javascript",
    "billy-herrington",
    "babel",
    "ts-morph"
  ],
  "author": "damilurg",
  "license": "MIT",
  "dependencies": {
    "@gachiscript/dictionary": "^1.0.0",
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "glob": "^10.3.0",
    "chokidar": "^3.5.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/babel__generator": "^7.6.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "eslint": "^8.55.0"
  }
}