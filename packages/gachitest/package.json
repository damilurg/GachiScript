{
  "name": "@gachiscript/gachitest",
  "version": "1.0.0",
  "description": "ðŸ”¥ GachiTest - Jest wrapper for testing in GachiScript style",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "gachitest": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts src/cli.ts --watch --format esm",
    "build": "tsup src/index.ts src/cli.ts --dts --format esm",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "gachiscript",
    "testing",
    "jest",
    "billy-herrington",
    "tdd",
    "test-runner"
  ],
  "author": "damilurg",
  "license": "MIT",
  "dependencies": {
    "@gachiscript/dictionary": "^1.0.0",
    "@gachiscript/transpiler": "^1.0.0",
    "jest": "^29.7.0",
    "@jest/core": "^29.7.0",
    "@jest/types": "^29.6.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "eslint": "^8.55.0"
  },
  "peerDependencies": {
    "jest": ">=29.0.0"
  }
}