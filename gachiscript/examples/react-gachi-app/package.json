{
  "name": "react-gachi-app",
  "version": "1.0.0",
  "description": "React application demonstrating GachiScript platform",
  "private": true,
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@gachiscript/dictionary": "1.0.0",
    "@gachiscript/transpiler": "1.0.0",
    "@gachiscript/gachitest": "1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "gachi build src --framework react && vite build",
    "build:gachi": "gachi build src --framework react -o src-gachi",
    "build:reverse": "gachi reverse src-gachi --framework react -o src-restored",
    "test": "gachitest run",
    "test:watch": "gachitest run --watch",
    "test:coverage": "gachitest run --coverage"
  },
  "keywords": [
    "react",
    "gachiscript",
    "billy-herrington",
    "demo"
  ],
  "author": "damilurg",
  "license": "MIT"
}